@model CreateAboutDto

<div class="section-wrapper">
    <form asp-action="Add" method="post">
        <div class="form-group">
            <label asp-for="Description" class="form-label">Content</label>
            <textarea asp-for="Description" class="form-control" rows="10"></textarea>
            <span asp-validation-for="Description"></span>
        </div>
        <button type="submit" class="btn btn-outline-success">About Create</button>
    </form>
</div>